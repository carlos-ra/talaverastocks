{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "jest",
      "node"
    ],
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.test.ts",
    "**/*.test.tsx",
    "jest.setup.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
